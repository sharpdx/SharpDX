		<Reference Include="System.Core"/>
		<Reference Include="System.Xml.Linq"/>
		<Reference Include="System.Data.DataSetExtensions"/>
		$endif$
		$if$ ($targetframeworkversion$ >= 4.0)
		<Reference Include="Microsoft.CSharp"/>
 		$endif$
		<Reference Include="System.Data"/>
		<Reference Include="System.Xml"/>
	</ItemGroup>
	<ItemGroup>
    <Compile Include="$safeclassname$.cs"/>
    <Compile Include="Program.cs" />
    <Compile Include="Properties\AssemblyInfo.cs" />
	</ItemGroup>
  <ItemGroup>
    $if$ ($sharpdx_feature_spritefont$ == true)
    <ToolkitFont Include="Content\Arial16.xml"/>
    $endif$
    $if$ ($sharpdx_feature_spritetexture$ >= 4.0)
    <ToolkitTexture Include="Content\Balls.dds"/>
    $endif$
    $if$ ($sharpdx_feature_model3d$ >= 4.0)
    <ToolkitModel Include="Content\Ship.fbx"/>
    <ToolkitTexture Include="Content\ShipDiffuse.dds"/>
    $endif$
    $if$ ($sharpdx_feature_bloomeffect$ == true)
    <ToolkitFxc Include="Content\Bloom.fx" />
    $endif$
  </ItemGroup>
  $if$ ($sharpdx_platform_desktop$ == true)
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  $endif$
