    <!--
    // *****************************************************************
    // D3D1x Interfaces
    // *****************************************************************
    -->
    <map interface="ID3D1[0-3](.+)" name-tmp="$1" />
    <map interface="ID3DX1[0-3](.+)" name-tmp="$1" />

    <map method=".*::GetPrivateData" check="false"/>

    <map interface="ID3D10Multithread" name="DeviceMultithread"/>

